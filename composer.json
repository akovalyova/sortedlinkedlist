{
    "name": "anastasiiakovalova/sortedlinkedlisttest",
    "config": {
        "platform": {
            "php": "8.3.0"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "squizlabs/php_codesniffer": "^3.11",
        "phpunit/phpunit": "^11.4"
    },
    "scripts": {
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs --standard=PSR12 src",
        "phpcbf": "phpcbf --standard=PSR12 src",
        "test": "phpunit"
    },
    "version": "1.0.0"
}
